  <%services_offered.each do |service|%>
  <div id="<%=service.title%>" class="col mt-3 mb-3">
    <div class="card-services">
      <div class="card-body">
        <a tabindex="0" role="img" data-bs-toggle="popover" data-trigger="hover" title="<%=service.title%>" 
          data-content="<%=service.content%>"<i id="services" class='<%=service.icon%>'></i></a>
        <h4 class="card-text pt-2"><%=service.title%></h4>
      </div>
    </div>
  </div>
  <% end %>
